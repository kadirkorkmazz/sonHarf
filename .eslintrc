{
  "parser": "babel-eslint",
  "extends": ["airbnb-base","prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prefer-destructuring": ["error", {"object": true, "array": true}],
    "no-var": ["error"],
    "prettier/prettier": ["error"],
    "no-console":"off",
    "import/no-extraneous-dependencies": "off"
    
  }
}